  "description": "  ",
  "keywords": [],
  "repository": "https://github.com/atom-ide-community/atom-ide-template",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix --ext js,ts,coffee,json",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "tsc": "tsc -p src/tsconfig.json || echo done",
    "dev": "npm run clean && env NODE_ENV=development env BABEL_ENV=production rollup -c -w",
    "build": "npm run clean && env NODE_ENV=production env BABEL_ENV=production rollup -c ",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./spec/runner",
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "etch": "^0.14.0",
    "atom-package-deps": "^6.0.0"
  },
  "devDependencies": {
    "@types/atom": "latest",
    "@types/node": "^13.11.1",
    "typescript": "^3.9.5",
    "tslib": "^2.0.0",
    "coffeescript": "^1.12.7",
    "jasmine": "^3.5.0",
    "@types/jasmine": "^3.5.10",
    "atom-jasmine3-test-runner": "^4.4.4",
    "prettier": "^2.0.5",
    "eslint": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-eslint": "latest",
    "eslint-plugin-coffee": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-only-warn": "latest",
    "eslint-config-prettier": "latest",
