{
  "name": "atom-ide-template",
  "main": "dist/main.js",
  "version": "0.1.0",
  "author": "<Your Name>",
  "description": "  ",
  "keywords": [],
  "repository": "https://github.com/atom-ide-community/atom-ide-template",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "tsc": "tsc -p src/tsconfig.json || echo done",
    "dev": "cross-env NODE_ENV=development parcel watch --target main src/main.ts",
    "build": "cross-env NODE_ENV=production parcel build --target main src/main.ts",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u -x coffeescript",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./spec/runner",
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^6.0.0"
  },
  "devDependencies": {
    "@types/atom": "1.40.4",
    "@types/node": "^14.6.0",
    "typescript": "^3.9.7",
    "tslib": "^2.0.1",
    "coffeescript": "^1.12.7",
    "@types/jasmine": "^3.5.12",
    "atom-jasmine3-test-runner": "^5.0.5",
    "parcel": "^2.0.0-beta.1",
    "@parcel/transformer-typescript-tsc": "^2.0.0-beta.1",
    "prettier": "^2.0.5",
    "eslint": "7.7.0",
    "eslint-config-atomic": "^1.4.0",
    "shx": "0.3.2",
    "cross-env": "7.0.2",
    "npm-check-updates": "7.1.1",
    "build-commit": "0.1.1"
  },
  "targets": {
    "main": {
      "context": "electron-renderer",
      "engines": {
        "electron": ">=5.x"
      },
      "includeNodeModules": {
        "atom": false,
        "electron": false,
        "coffee-script": false
      },
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  },
  "providedServices": {},
  "consumedServices": {},
  "package-deps": []
}
