    "coffeescript": "^2.5.1",
    "jasmine": "^3.5.0",
    "@types/jasmine": "^3.5.11",
    "atom-jasmine3-test-runner": "^4.4.8",
    "prettier": "^2.0.5",
    "eslint": "7.4.0",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "babel-eslint": "10.1.0",
